$(RUBYLIBDIR)/ripper/filter.rb: $(srcdir)/lib/ripper/filter.rb $(TIMESTAMP_DIR)/.RUBYCOMMONDIR.-.ripper.time
	$(Q) $(COPY) $(srcdir)/lib/ripper/filter.rb $(@D)
