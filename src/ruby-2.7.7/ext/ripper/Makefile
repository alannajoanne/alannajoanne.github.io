$(RUBYLIBDIR)/ripper/lexer.rb: $(srcdir)/lib/ripper/lexer.rb $(TIMESTAMP_DIR)/.RUBYCOMMONDIR.-.ripper.time
	$(Q) $(COPY) $(srcdir)/lib/ripper/lexer.rb $(@D)
